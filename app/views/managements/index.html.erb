<p id="notice"><%= notice %></p>

<h1>Managements</h1>

<table>
  <thead>
    <tr>
      <th>Present</th>
      <th>In time</th>
      <th>Out time</th>
      <th>Target</th>
      <th>Page processed</th>
      <th>Page error</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @managements.each do |management| %>
      <tr>
        <td><%= management.present %></td>
        <td><%= management.in_time %></td>
        <td><%= management.out_time %></td>
        <td><%= management.target %></td>
        <td><%= management.page_processed %></td>
        <td><%= management.page_error %></td>
        <td><%= link_to 'Show', management %></td>
        <td><%= link_to 'Edit', edit_management_path(management) %></td>
        <td><%= link_to 'Destroy', management, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Management', new_management_path %>
